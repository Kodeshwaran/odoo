<odoo>
    <data>
        <record id="seq_customer_code" model="ir.sequence">
            <field name="name">Customer Code Sequence</field>
            <field name="code">res.partner</field>
            <field name="prefix">CUS</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_opportunity_no" model="ir.sequence">
            <field name="name">Opportunity Number Sequence</field>
            <field name="code">crm.lead</field>
            <field name="prefix"></field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>