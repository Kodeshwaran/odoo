<odoo>
    <record id="seq_project_seq" model="ir.sequence">
        <field name="name">Project</field>
        <field name="code">project.proj</field>
        <field name="prefix">PR/</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="seq_project_task" model="ir.sequence">
            <field name="name">Task</field>
            <field name="code">project.task</field>
            <field name="prefix">PT/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

</odoo>

