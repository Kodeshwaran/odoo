<odoo>

    <data>
        <record id="sale_quotation_seq" model="ir.sequence">
            <field name="name">Sale Quotation Sequence</field>
            <field name="code">sale.quotation</field>
            <field name="prefix">SQ/2023-24/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

    <record id="sale_order_pro_forma_seq" model="ir.sequence">
        <field name="name">Pro-Forma Sequence</field>
        <field name="code">pro.forma</field>
        <field name="prefix">PRO/%(year)s/</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>

</odoo>