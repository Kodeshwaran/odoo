<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>

        <record id="epo_type_inherit_form_view" model="ir.ui.view">
            <field name="name">epo type inherit form view</field>
            <field name="model">epo.type</field>
            <field name="inherit_id" ref="rims_dashboard.epo_type_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='mt_alert']" position="before">
                    <field name="amount"/>
                    <field name="service_type_id"/>
                </xpath>
            </field>
        </record>

    </data>
</odoo>
